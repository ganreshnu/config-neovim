{
	"$schema": "https://raw.githubusercontent.com/sumneko/vscode-lua/master/setting/schema.json",
	"runtime.version": "LuaJIT",
	"runtime.pathStrict": true,
	"workspace.library": [
		"$VIMRUNTIME",
		"$VIMRUNTIME/ftplugin",
		"$VIMRUNTIME/autoload",
		"$VIMRUNTIME/plugin",
		"$VIMRUNTIME/indent",
		"$VIMRUNTIME/syntax",
		"$VIMRUNTIME/lua",
		"$VIMRUNTIME/lua/vim",
		"$VIMRUNTIME/lua/vim/filetype",
		"$VIMRUNTIME/lua/vim/lsp",
		"$VIMRUNTIME/lua/vim/treesitter",
		"$VIMRUNTIME/lua/nvim",
		"$HOME/.local/share/nvim/lazy/Comment.nvim/lua",
		"$HOME/.local/share/nvim/lazy/mason.nvim/lua"
	]
}
